                @Html.DisplayFor(modelItem => item.Line1)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Line2)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Line3)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Line4)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Line5)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.isBilling)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.isPrimary)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CustEntMain.Id)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
